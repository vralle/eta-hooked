{
  "name": "@vralle/eta-hoked",
  "version": "0.1.0",
  "description": "Fork of Eta.js with hookable load content transformers.",
  "license": "MIT",
  "keywords": [
    "eta",
    "template-engine"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vralle/eta-hooked.git"
  },
  "type": "module",
